<Window x:Class="SitStandLean.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SitStandLean"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920" WindowStyle="None" WindowState="Maximized">
    <Viewbox>
        <Grid Width="1920" Height="1080">
            <Image Name="camera" />
            <Canvas Name="canvas">
                <Image Name="maskImage" Source="Images/tinfoil.png" IsEnabled="False" Visibility="Hidden" />
                <Image Name="maskImage2" Source="Images/cowboy.png" IsEnabled="False" Visibility="Hidden" />
                <Image Name="maskImage3" Source="Images/napolean.png" IsEnabled="False" Visibility="Hidden" />
            </Canvas>
        </Grid>
    </Viewbox>
</Window>
